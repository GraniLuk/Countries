<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Assert.That(actual, Is.InRange(from, to))</Title>
    <Author>Piotr Perak</Author>
    <Shortcut>atiir</Shortcut>
    <Description>Shortcut for Is.InRange assertion</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>actual</ID>
        <Default>actual</Default>
      </Literal>
      <Literal>
        <ID>from</ID>
        <Default>from</Default>
      </Literal>
      <Literal>
        <ID>to</ID>
        <Default>to</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp">
      <![CDATA[Assert.That($actual$, Is.InRange($from$, $to$));
      $end$]]>
    </Code>
  </Snippet>
</CodeSnippet>