<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Test TearDown</Title>
    <Author>Piotr Perak</Author>
    <Shortcut>tt</Shortcut>
    <Description>Shortcut for generating NUnit test teardown method</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>TearDown</ID>
        <Default>TearDown</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp">
      <![CDATA[[TearDown]
      public void $TearDown$()
      {
	  	$end$
      }
      ]]>
    </Code>
  </Snippet>
</CodeSnippet>
