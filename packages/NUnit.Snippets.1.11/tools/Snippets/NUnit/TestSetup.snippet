<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Test setup</Title>
    <Author>Piotr Perak</Author>
    <Shortcut>ts</Shortcut>
    <Description>Shortcut for generating NUnit test setup method</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>Setup</ID>
        <Default>Setup</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp">
      <![CDATA[[SetUp]
      public void $Setup$()
      {
	  	$end$
      }
      ]]>
    </Code>
  </Snippet>
</CodeSnippet>
