<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Assert.That(actual, Has.Member(expected))</Title>
    <Author>Piotr Perak</Author>
    <Shortcut>athm</Shortcut>
    <Description>Shortcut for Has.Member assertion</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>actual</ID>
        <Default>actual</Default>
      </Literal>
      <Literal>
        <ID>expected</ID>
        <Default>expected</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp">
      <![CDATA[Assert.That($actual$, Has.Member($expected$));
      $end$]]>
    </Code>
  </Snippet>
</CodeSnippet>